//subworkflows_RunMultiCovTranscript
nextflow.enable.dsl=2

//import modules
include { runMultiCovTranscript } from '../modules/bedtools_multicov'

workflow RunMultiCovTranscript {
  take:
  sortedBamfiles
  bamfilesindex
  
  main:
  runMultiCovTranscript(sortedBamfiles, bamfilesindex)
  
  emit:
  runMultiCovTranscript.out
}
